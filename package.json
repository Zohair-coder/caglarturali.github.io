{
  "name": "developer-portfolio-app",
  "version": "2.0.0",
  "description": "VS Code/Atom One Dark inspired developer portfolio app.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Zohair-coder/caglarturali.github.io"
  },
  "homepage": "https://www.zohair.dev/",
  "license": "GNU GPLv3",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test:base": "react-scripts test --passWithNoTests",
    "test": "craco test",
    "test:watch": "npm run test:base -- --watchAll",
    "test:update": "npm run test:base -- --updateSnapshot --forceExit",
    "eject": "react-scripts eject",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\"",
    "lint:ci": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "progress": "ts-node -O '{\"module\":\"commonjs\"}' scripts/updateProgress.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@reach/router": "^1.3.4",
    "clsx": "^1.1.1",
    "color": "^3.2.1",
    "dayjs": "^1.10.7",
    "isomorphic-unfetch": "^3.1.0",
    "normalize.css": "^8.0.1",
    "posthog-js": "^1.215.1",
    "react": "^16.14.0",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-jss": "^10.8.2",
    "react-markdown": "^4.3.1",
    "react-scripts": "^5.0.1",
    "react-tooltip": "^4.2.21",
    "rehype-raw": "^6.1.1",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.8.3",
    "@types/color": "^3.0.2",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.17.32",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^16.14.20",
    "@types/react-dom": "^16.9.14",
    "@types/react-helmet": "^5.0.19",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "path-browserify": "^1.0.1",
    "prettier": "^2.4.1",
    "process": "^0.11.10",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^3.9.10",
    "webpack": "^5.97.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
